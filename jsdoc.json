{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
      "include": ["./lib", "./node_modules/influx-ql/lib"],
      "includePattern": ".js$",
      "excludePattern": "(http.js|utils.js|influx.js)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "static": true
  }
}
